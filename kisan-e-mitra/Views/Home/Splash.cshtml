@{
    ViewBag.Title = Resources.Resource.label_title;
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Welcome to Kisan e-Mitra</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/Content/splash.css"/>
    @Scripts.Render("~/bundles/jquery")
</head>
<body class="splash-body">
    @Html.Hidden("RedirectFromSplash", Url.Action("Index", "Home"))
    <div>
        <img src="~/Content/images/MOA_logo.png" alt="welcome logo" width="200" height="200" />
    </div>
    <div>
        <h1 class="splas-header">@Resources.Resource.label_title</h1>
    </div>

    <script type="text/javascript" src="~/Content/js/splash.js">

    </script>
</body>
</html>
